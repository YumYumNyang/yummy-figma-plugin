{
  "name": "global-style-to-code",
  "version": "1.0.0",
  "description": "extract all styling from figma files",
  "scripts": {
    "start": "parcel src/ui.html src/code.ts  --no-source-maps --no-cache --no-hmr",
    "build": "parcel build --no-source-maps  --no-cache"
  },
  "source": [
    "src/code.ts",
    "src/ui.html"
  ],
  "target": {
    "default": {
      "source": [
        "src/code.ts",
        "src/ui.html"
      ]
    }
  },
  "author": "hyewon kwak",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@figma/plugin-typings": "*",
    "parcel": "2.8.0",
    "typescript": "^4.3.5"
  }
}
